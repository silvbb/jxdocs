{
  "name": "@elog/doc",
  "version": "1.0.0",
  "description": "Elog文档",
  "homepage": "https://github.com/LetTTGACO/elog/tree/master/packages/elog-doc#readme",
  "author": "1874 <me@mail.1874.cool>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LetTTGACO/elog.git"
  },
  "bugs": {
    "url": "https://github.com/LetTTGACO/elog/issues"
  },
  "scripts": {
    "//docs:dev": "vitepress dev docs --config vite.config.ts --debug",
    "///docs:dev": "set DEBUG=vitepress:* && set VITEPRESS_LOG=true && vitepress dev docs",
    "docs:dev": "set DEBUG=vitepress:* && set VITEPRESS_DEBUG=true && vitepress dev docs",
    "/docs:dev": "cross-env DEBUG=vitepress:* VITEPRESS_DEBUG=true pnpm vitepress dev docs",
    "docs:build": "vitepress build docs",
    "//docs:build": "set DEBUG=vitepress:* && set VITEPRESS_LOG=true && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "elog:init": "elog init -c elog.config.init.js -e .elog.init.env",
    "local:feishu": "elog sync -c elog.config.feishu.js -e .elog.env  -a docs/feishu/elog.cache.feishu.json",
    "sync:feishu": "elog sync -c elog.config.feishu.js -a elog.cache.feishu.json",
    "local:feishu-wiki": "elog sync -c elog.config.feishu-wiki.js -e .elog.env  -a docs/feishu-wiki/elog.cache.feishu-wiki.json",
    "sync:feishu-wiki": "elog sync -c elog.config.feishu-wiki.js -a elog.cache.feishu-wiki.json",
    "local:notion": "elog sync -c elog.config.notion.cjs -e .elog.env -a docs/notion/elog.cache.notion.json",
    "sync:notion": "elog sync -c elog.config.notion.js -a docs/notion/elog.cache.notion.json",
    "local:yuque-token": "elog sync -c elog.config.yuque-token.js -e .elog.env -a docs/yuque/elog.cache.yuque-token.json",
    "local:yuque-pwd": "elog sync -c elog.config.yuque-pwd.js -e .elog.env -a docs/yuque-pwd/elog.cache.yuque-pwd.json",
    "local:wolai": "elog sync -c elog.config.wolai.js -e .elog.env -a docs/wolai/elog.cache.wolai.json",
    "local:flowus": "elog sync -c elog.config.flowus.js -e .elog.env  -a docs/flowus/elog.cache.flowus.json",
    "local:wordpress": "elog sync -c elog.config.wordpress.js -e .elog.env  -a docs/wordpress/elog.cache.wordpress.json",
    "local:halo": "elog sync -c elog.config.halo.js -e .elog.env  -a docs/halo/elog.cache.halo.json",
    "local:outline": "elog sync -c elog.config.outline.js -e .elog.env  -a docs/outline/elog.cache.outline.json",
    "sync:yuque-token": "elog sync -c elog.config.yuque-token.js -a elog.cache.yuque-token.json",
    "sync:yuque-pwd": "elog sync -c elog.config.yuque-pwd.js -a elog.cache.yuque-pwd.json",
    "sync:flowus": "elog sync -c elog.config.flowus.js -a elog.cache.flowus.json",
    "sync:wordpress": "elog sync -c elog.config.wordpress.js -a elog.cache.wordpress.json",
    "sync:halo": "elog sync -c elog.config.halo.js -a elog.cache.halo.json",
    "sync:outline": "elog sync -c elog.config.outline.js -a elog.cache.outline.json",
    "clean:yuque-token": "elog clean -c elog.config.yuque-token.js -a elog.cache.yuque-token.json",
    "clean:yuque-pwd": "elog clean -c elog.config.yuque-pwd.js -a elog.cache.yuque-pwd.json",
    "clean:notion": "elog clean -c elog.config.notion.js -a elog.cache.notion.json",
    "clean:flowus": "elog clean -c elog.config.flowus.js  -a elog.cache.flowus.json",
    "clean:wordpress": "elog clean -c elog.config.wordpress.js  -a elog.cache.wordpress.json",
    "clean:halo": "elog clean -c elog.config.halo.js  -a elog.cache.halo.json",
    "clean:feishu": "elog clean -c elog.config.feishu.js  -a elog.cache.feishu.json",
    "clean:feishu-wiki": "elog clean -c elog.config.feishu-wiki.js  -a elog.cache.feishu-wiki.json",
    "clean-all": "npm run clean:yuque-token && npm run clean:notion && npm run clean:flowus && npm run clean:wordpress && npm run clean:feishu && npm run clean:feishu-wiki",
    "sync-all": "npm run local:yuque-token && npm run local:notion && npm run local:flowus && npm run local:wordpress && npm run local:feishu && npm run local:feishu-wiki",
    "format": "prettier --write \"docs/**/*.{js,ts,md,vue}\""
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.53.3",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/vscode-icons": "^1.2.16",
    "@mdit-vue/shared": "^1.0.0",
    "@shikijs/vitepress-twoslash": "^2.5.0",
    "@types/blueimp-md5": "^2.18.2",
    "@types/debug": "^4.1.12",
    "@types/jquery": "^3.5.29",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.19.4",
    "@unocss/preset-attributify": "66.1.0-beta.6",
    "@unocss/preset-icons": "66.1.0-beta.6",
    "@unocss/preset-wind": "66.1.0-beta.6",
    "@unocss/transformer-directives": "66.1.0-beta.6",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vueuse/core": "^10.7.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-mathjax3": "^4.3.2",
    "mermaid": "9.3.0",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.75.0",
    "tailwindcss": "3.4.3",
    "unocss": "66.1.0-beta.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.3",
    "vitepress": "^1.0.0-rc.31",
    "vitepress-plugin-group-icons": "^1.3.8",
    "vitepress-plugin-mermaid": "2.0.8",
    "vue": "^3.3.9"
  },
  "dependencies": {
    "@antv/g2plot": "2.4.31",
    "@elog/cli": "0.15.0-beta.0",
    "@giscus/vue": "^3.0.0",
    "aegis-web-sdk": "^1.39.2",
    "axios": "^1.6.2",
    "blueimp-md5": "^2.19.0",
    "d3-interpolate": "3.0.1",
    "dayjs": "^1.11.10",
    "element-plus": "^2.3.14",
    "fast-glob": "^3.3.2",
    "gitalk": "^1.8.0",
    "gray-matter": "^4.0.3",
    "jquery": "^3.7.1",
    "markdown-it-task-lists": "^2.1.1",
    "medium-zoom": "^1.1.0",
    "sitemap": "^7.1.1",
    "vitepress": "^1.0.0-rc.40",
    "vitepress-plugin-comment-with-giscus": "^1.1.12",
    "xlsx": "^0.18.5",
    "markdown-it-mathjax3": "^4.3.2",
    "markdown-it-footnote": "^3.0.3"
  },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
